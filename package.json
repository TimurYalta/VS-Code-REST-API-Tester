{
	"name": "rest-api-tester",
	"displayName": "Rest API Tester",
	"description": "Extension allows to test REST API queries live in VS code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.sendRequest",
		"onCommand:extension.sendGET",
		"onCommand:extension.sendPOST"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.sendRequest",
				"title": "Send Request"
			},
			{
				"command": "extension.sendGET",
				"title": "Send GET request"
			},
			{
				"command": "extension.sendPOST",
				"title": "Send POST request"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.sendRequest",
					"group": "YourGroup@1"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"node-fetch": "^2.3.0"
	}
}
